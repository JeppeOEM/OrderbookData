{
  "name": "order-data",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/order-data/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "typeorm:migration:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx typeorm migration:generate src/database/migrations/{args.name} -d typeorm.config.ts",
        "cwd": "{projectRoot}"
      }
    },
    "typeorm:migration:run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx typeorm migration:run -d typeorm.config.ts",
        "cwd": "{projectRoot}"
      }
    },
    "typeorm:migration:revert": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx typeorm migration:revert -d typeorm.config.ts",
        "cwd": "{projectRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/order-data"],
      "options": {
        "command": "tsc --project apps/order-data/tsconfig.app.json --outDir dist/apps/order-data"
      },
      "configurations": {
        "production": {},
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/order-data/package.json",
        "{workspaceRoot}/dist/apps/order-data/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": ["{workspaceRoot}/dist/apps/order-data/workspace_modules"],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "order-data:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "order-data:build:development"
        },
        "production": {
          "buildTarget": "order-data:build:production"
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
